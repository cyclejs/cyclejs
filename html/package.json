{
  "name": "@cycle/html",
  "version": "1.1.0",
  "description": "A driver for HTML strings based on Snabbdom and the DOM driver",
  "license": "MIT",
  "homepage": "https://cycle.js.org",
  "bugs": "https://github.com/cyclejs/cyclejs/issues",
  "repository": "https://github.com/cyclejs/cyclejs/tree/master/html",
  "contributors": [
    {
      "name": "Andre Staltz",
      "email": "andre@staltz.com"
    },
    {
      "name": "Tylor Steinberger",
      "email": "tlsteinberger167@gmail.com"
    },
    {
      "name": "Frederik Krautwald",
      "email": "fkrautwald@gmail.com"
    }
  ],
  "keywords": [
    "reactive",
    "framework",
    "rxjs",
    "rx",
    "unidirectional",
    "mvi",
    "virtual-dom"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@cycle/run": "*",
    "@cycle/dom": "17.x",
    "snabbdom": "0.6.9",
    "snabbdom-to-html": "3.1.1"
  },
  "peerDependencies": {
    "xstream": "*"
  },
  "devDependencies": {
    "@cycle/isolate": "2.1.0",
    "@types/mocha": "^2.2.33",
    "@types/node": "^7.0.4",
    "@types/sinon": "^1.16.32",
    "saucie": "3.1.0",
    "simulant": "^0.2.2",
    "snabbdom-jsx": "^0.3.0",
    "xstream": "10.x"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "browserify-shim": {
    "xstream": "global:xstream"
  },
  "scripts": {
    "test-node": "../node_modules/.bin/mocha test/*.ts --require ts-node/register",
    "test": "npm run test-node",
    "test-ci": "npm run test",
    "browserify": "../node_modules/.bin/browserify lib/index.js --global-transform=browserify-shim --standalone CycleHTML --exclude xstream --outfile dist/cycle-html.js",
    "minify": "node ../.scripts/minify.js dist/cycle-html.js dist/cycle-html.min.js"
  }
}
