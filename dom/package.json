{
  "name": "@cycle/dom",
  "version": "22.8.0",
  "description": "The standard DOM Driver for Cycle.js, based on Snabbdom",
  "license": "MIT",
  "homepage": "https://cycle.js.org",
  "bugs": "https://github.com/cyclejs/cyclejs/issues",
  "repository": "https://github.com/cyclejs/cyclejs/tree/master/dom",
  "contributors": [
    {
      "name": "Andre Staltz",
      "email": "andre@staltz.com"
    },
    {
      "name": "Jan van Br√ºgge",
      "email": "supermanitu@gmail.com"
    }
  ],
  "keywords": [
    "reactive",
    "framework",
    "rxjs",
    "rx",
    "unidirectional",
    "mvi",
    "virtual-dom"
  ],
  "main": "build/bundle.cjs.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "rm -rf build ; tsc --build tsconfig.build.json && pnpm run build:cjs",
    "build:cjs": "BUNDLE_FORMAT=cjs rollup -c ../configs/rollup.config.ts",
    "test": "pnpm run test:node && pnpm run test:browser",
    "test:ci": "pnpm run test",
    "test:browser": "karma start",
    "test:node": "mocha --require ts-node/register test/node/mockDomPlugin.ts"
  },
  "dependencies": {
    "@cycle/callbags": "^4.1.0",
    "snabbdom": "^3.5.0"
  },
  "peerDependencies": {
    "@cycle/run": "^6.0.0"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/cyclejs"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@cycle/run": "workspace:^6.0.0",
    "@cycle/utils": "workspace:^6.0.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "fast-check": "^2.25.0",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "^3.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-spec-reporter": "^0.0.34",
    "karma-typescript": "^5.5.3",
