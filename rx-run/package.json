{
  "name": "cyclejs",
  "version": "0.5.2",
  "author": "Andre Staltz",
  "description": "A web application framework with Model-View-Intent and Virtual DOM",
  "license": "MIT",
  "homepage": "http://staltz.com/cycle",
  "bugs": "https://github.com/staltz/cycle/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/staltz/cycle"
  },
  "keywords": [
    "mvi",
    "virtual-dom"
  ],
  "main": "src/cycle.js",
  "dependencies": {
    "rx": "2.3.18",
    "virtual-dom": "0.0.22",
    "virtual-hyperscript": "4.5.0",
    "dom-delegator": "11.0.0"
  },
  "devDependencies": {
    "browserify": "6.3.2",
    "jshint": "^2.5.6",
    "jscs": "^1.6.2",
    "uglify-js": "^2.4.15",
    "mocha": "^2.0.1",
    "markdox": "^0.1.7"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "lint": "jshint src/ ",
    "jscs": "jscs src/",
    "test": "mocha",
    "check": "npm run lint && npm run jscs && npm run test",
    "browserify": "browserify src/for-browserify.js --igv=Cycle -o dist/cycle.js",
    "uglify": "uglifyjs dist/cycle.js -o dist/cycle.min.js",
    "docs": "node ./scripts/make-api-docs.js",
    "build": "npm run browserify && npm run uglify"
  }
}
